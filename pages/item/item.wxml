<!--pages/item/item.wxml-->
<view class="itemPage">  
    <navigator wx:for="{{itemList}}" wx:key="{{index}}" class="itemList" url="{{'../itemdetail/itemdetail?idx='+item.idx}}">
      <image src="{{item.src}}" class="itemImg"></image>
      <text class="itemText">{{item.itemTitle}}</text>
      <text class="jianjiaohao">></text>
    </navigator>
</view>